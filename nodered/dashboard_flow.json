        "id": "alarm-table",
        "type": "ui_template",
        "z": "dashboard-tab",
        "group": "ui_group_alarms",
        "name": "Alarm Display",
        "order": 1,
        "width": "12",
        "height": "4",
        "format": "<div style=\"padding: 10px; background: #fff3cd; border-left: 4px solid #ff9800; margin-bottom: 10px;\">\n    <h3 style=\"margin: 0 0 10px 0; color: #ff6600;\">‚ö†Ô∏è ALARM: {{msg.payload.node_id}} ({{msg.payload.location}})</h3>\n    <p style=\"margin: 5px 0; font-size: 12px; color: #666;\">{{msg.payload.timestamp}}</p>\n    <ul style=\"margin: 10px 0; padding-left: 20px;\">\n        <li ng-repeat=\"violation in msg.payload.violations\" style=\"margin: 5px 0; color: #d32f2f;\">\n            {{violation}}\n        </li>\n    </ul>\n    <div style=\"margin-top: 10px; padding: 8px; background: #f5f5f5; border-radius: 4px; font-size: 11px;\">\n        <strong>Current Values:</strong>\n        Temp: {{msg.payload.original_data.temperature}}¬∞C | \n        Humidity: {{msg.payload.original_data.humidity}}% | \n        Soil: {{msg.payload.original_data.soil}}% | \n        Light: {{msg.payload.original_data.light}}% | \n        Air: {{msg.payload.original_data.air_quality}}\n    </div>\n</div>",
        "storeOutMessages": true,
        "fwdInMessages": true,
        "resendOnRefresh": true,
        "templateScope": "local",
